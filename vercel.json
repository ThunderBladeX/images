{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": [
          "../templates/**",
          "../alembic.ini",
          "../alembic/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon\\.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/favicon\\.png",
      "dest": "/favicon.png"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
